				<code_for_machine_translation>pt-PT</code_for_machine_translation>
			</custom_language>
		</custom_languages>
		<id_extractor_ignores/>
	</editor_configuration>
	<primary_language/>
	<configuration>
		<indent>tab</indent>
		<format>json</format>
		<support_arrays>true</support_arrays>
